{
  "compilerOptions": {
    "target": "esnext",
    "lib": ["es6", "dom"],
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "jsx": "react-native",
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "isolatedModules": true,
    "noEmit": true,
    "module": "esnext",
    "forceConsistentCasingInFileNames": true
  },
  "extends": "./node_modules/expo/tsconfig.base"
}
